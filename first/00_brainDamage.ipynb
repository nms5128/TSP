    "def fac(x):\n",
    "    assert (x >= 1)\n",
    "    def underhood(acc, x):\n",
    "        if x == 1:\n",
    "            return(acc)\n",
    "        else:\n",
    "            return(underhood(acc*x, x-1))\n",
    "    ##\n",
    "    return(underhood(1, x))\n",
    "\n",
    "fac(11)"
   ]
